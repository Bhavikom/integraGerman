<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:gravity="center"
            android:orientation="horizontal"
            android:padding="5dp">

            <TextView
                android:id="@+id/labelAuthOwner"
                style="@style/TextView_Black"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Auth-owner" />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center">

                <ToggleButton
                    android:id="@+id/button1"
                    style="@style/TextView_White"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/toggle_selector"
                    android:checked="true"
                    android:textOff=""
                    android:textOn="" />
            </LinearLayout>

            <TextView
                android:id="@+id/labelName"
                style="@style/TextView_Black"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_weight="0.5"
                android:text="Name" />

            <EditText
                android:id="@+id/textName"
                style="@style/EditText_bg"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:singleLine="true"
                android:maxLength="40"
                android:imeOptions="actionNext"
                android:layout_weight="1.5" />

            <TextView
                android:id="@+id/labelAnsbach"
                style="@style/TextView_Black"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_weight="0.5"
                android:text="Ansbach." />

            <EditText
                android:id="@+id/textAnsbach"
                style="@style/EditText_bg"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:singleLine="true"
                android:maxLength="40"
                android:imeOptions="actionNext"
                android:layout_weight="1.5" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal"
            android:padding="5dp">

            <TextView
                android:id="@+id/labelPhone"
                style="@style/TextView_Black"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Phone" />

            <EditText
                android:id="@+id/textTelephone"
                style="@style/EditText_bg"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:inputType="phone"
                android:singleLine="true"
                android:maxLength="25"
                android:imeOptions="actionNext"
                android:layout_weight="1" />



            <TextView
                android:id="@+id/labelFaxNo"
                style="@style/TextView_Black"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_weight="0.5"
                android:text="Fax No." />

            <EditText
                android:id="@+id/textFaxNo"
                style="@style/EditText_bg"
                android:layout_width="0dp"
                android:inputType="phone"
                android:singleLine="true"
                android:maxLength="25"
                android:imeOptions="actionNext"
                android:layout_height="wrap_content"
                android:layout_weight="1.5" />

            <TextView
                android:id="@+id/labelIsFord"
                style="@style/TextView_Black"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_weight="0.5"
                android:text="is ford." />

            <EditText
                android:id="@+id/textIsFord"
                style="@style/EditText_bg"
                android:layout_width="0dp"
                android:singleLine="true"
                android:maxLength="80"
                android:layout_height="wrap_content"
                android:imeOptions="actionNext"
                android:layout_weight="1.5" />
        </LinearLayout>

        <LinearLayout
            style="@style/padding_style"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/labelAuthCommunity"
                style="@style/TextView_Black"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Auth-community" />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center">

                <ToggleButton
                    android:id="@+id/button2"
                    style="@style/TextView_White"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/toggle_selector"
                    android:checked="true"
                    android:textOff=""
                    android:textOn="" />
            </LinearLayout>

            <TextView
                android:id="@+id/labelAuthPt"
                style="@style/TextView_Black"
                android:layout_width="0dp"
                android:layout_marginLeft="10dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Auth-PT" />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="right">

                <ToggleButton
                    android:id="@+id/button3"
                    style="@style/TextView_White"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/toggle_selector"
                    android:checked="true"
                    android:textOff=""
                    android:textOn="" />
            </LinearLayout>

            <TextView
                android:id="@+id/labelAuthTaxiAssociation"
                style="@style/TextView_Black"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_weight="1"
                android:text="Auth-Taxi Association" />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="right">

                <ToggleButton
                    android:id="@+id/button4"
                    style="@style/TextView_White"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/toggle_selector"
                    android:checked="true"
                    android:textOff=""
                    android:textOn="" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            style="@style/padding_style"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/labelAbsperrmaß"
                style="@style/TextView_Black"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="Absperrmaß" />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center">

                <ToggleButton
                    android:id="@+id/button5"
                    style="@style/TextView_White"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/toggle_selector"
                    android:checked="true"
                    android:textOff=""
                    android:textOn="" />
            </LinearLayout>

            <TextView
                android:id="@+id/labelSketch"
                style="@style/TextView_Black"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_weight="1"
                android:text="Sketch" />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="right">

                <ToggleButton
                    android:id="@+id/button6"
                    style="@style/TextView_White"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/toggle_selector"
                    android:checked="true"
                    android:textOff=""
                    android:textOn="" />
            </LinearLayout>

            <TextView
                android:id="@+id/labelTrafficSignPlanRequired"
                style="@style/TextView_Black"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_weight="1"
                android:text="Traffic sign plan required" />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="right">

                <ToggleButton
                    android:id="@+id/button7"
                    style="@style/TextView_White"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/toggle_selector"
                    android:checked="true"
                    android:textOff=""
                    android:textOn="" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="5dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <LinearLayout
                    style="@style/EditText_bg"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.2"
                    android:gravity="center|left">

                    <TextView
                        style="@style/TextView_Black"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Lane Roadway"
                        android:id="@+id/labelLaneRoadway" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.1"
                    android:gravity="center"
                    style="@style/EditText_bg"
                    android:orientation="vertical">

                    <TextView

                        style="@style/EditText_bg"
                        android:layout_width="wrap_content"
                        android:layout_height="0dp"
                        android:layout_weight="0.5"
                        android:gravity="center"
                        android:text="Direction"
                        android:id="@+id/labelDirection" />

                    <TextView
                        style="@style/EditText_bg"
                        android:layout_width="wrap_content"
                        android:layout_height="0dp"
                        android:layout_weight="0.5"
                        android:gravity="center"
                        android:text="Number"
                        android:id="@+id/labelNumber" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.3"
                    android:gravity="center"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <EditText
                                android:id="@+id/textRichTung1"
                                android:layout_width="match_parent"
                                style="@style/EditText_bg"
                                android:singleLine="true"
                                android:imeOptions="actionNext"
                                android:maxLength="40"
                                android:layout_height="wrap_content" />

                        </LinearLayout>

                        <TextView
                            android:id="@+id/labelWidth"
                            android:text="Width[m]"
                            android:gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            style="@style/EditText_bg">

                        </TextView>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            style="@style/EditText_bg"
                            android:orientation="horizontal">

                            <TextView

                                style="@style/EditText_bg"
                                android:layout_width="0dp"
                                android:layout_weight="0.3"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="Lane"
                                android:id="@+id/labelLane" />

                            <TextView
                                style="@style/EditText_bg"
                                android:layout_width="0dp"
                                android:layout_weight="0.3"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="Go Away"
                                android:id="@+id/labelGoAway" />

                            <TextView
                                style="@style/EditText_bg"
                                android:layout_width="0dp"
                                android:layout_weight="0.3"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="Roadway"
                                android:id="@+id/labelRoadway" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="0.1"
                    android:gravity="center"
                    style="@style/EditText_bg"
                    android:orientation="vertical">

                    <TextView

                        style="@style/EditText_bg"
                        android:layout_width="wrap_content"
                        android:layout_height="0dp"
                        android:layout_weight="0.5"
                        android:gravity="center"
                        android:text="Direction"
                        android:id="@+id/labelDirection1" />

                    <TextView
                        style="@style/EditText_bg"
                        android:layout_width="wrap_content"
                        android:layout_height="0dp"
                        android:layout_weight="0.5"
                        android:gravity="center"
                        android:text="Number"
                        android:id="@+id/labelNumber1" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.3"
                    android:gravity="center"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <EditText
                                android:id="@+id/textRichTung2"
                                android:layout_width="match_parent"
                                style="@style/EditText_bg"
                                android:singleLine="true"
                                android:imeOptions="actionNext"
                                android:maxLength="40"
                                android:layout_height="wrap_content" />

                        </LinearLayout>

                        <TextView
                            android:id="@+id/labelWidth1"
                            android:text="Width[m]"
                            android:gravity="center"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            style="@style/EditText_bg">

                        </TextView>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            style="@style/EditText_bg"
                            android:orientation="horizontal">

                            <TextView

                                style="@style/EditText_bg"
                                android:layout_width="0dp"
                                android:layout_weight="0.3"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="Lane"
                                android:id="@+id/labelLane1" />

                            <TextView
                                style="@style/EditText_bg"
                                android:layout_width="0dp"
                                android:layout_weight="0.3"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="Go Away"
                                android:id="@+id/labelGoAway1" />

                            <TextView
                                style="@style/EditText_bg"
                                android:layout_width="0dp"
                                android:layout_weight="0.3"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:text="Roadway"
                                android:id="@+id/labelRoadway1" />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="0.2"
                    style="@style/EditText_bg"
                    android:gravity="center|left"
                    android:text="Available"
                    android:layout_height="wrap_content"
                    android:id="@+id/labelAvailable" />

                <EditText
                    android:id="@+id/textFahrstreifenAnzahl1"
                    android:layout_width="0dp"
                    android:layout_weight="0.1"
                    style="@style/EditText_bg"
                    android:singleLine="true"
                    android:inputType="phone"
                    android:digits="0123456789"
                    android:imeOptions="actionNext"
                    android:layout_height="wrap_content" />

                <EditText
                    android:id="@+id/textFahrbahnbreite1"
                    android:layout_width="0dp"
                    android:layout_weight="0.1"
                    style="@style/EditText_bg"
                    android:singleLine="true"
                    android:inputType="phone"
                    android:maxLength="4"
                    android:digits="0123456789,."
                    android:imeOptions="actionNext"
                    android:layout_height="wrap_content" />

                <EditText
                    android:id="@+id/textGehwegbreite1"
                    android:layout_width="0dp"
                    android:layout_weight="0.1"
                    style="@style/EditText_bg"
                    android:singleLine="true"
                    android:inputType="phone"
                    android:maxLength="4"
                    android:digits="0123456789,."
                    android:imeOptions="actionNext"
                    android:layout_height="wrap_content" />

                <EditText
                    android:id="@+id/textRadwegbreite1"
                    android:layout_width="0dp"
                    android:layout_weight="0.1"
                    style="@style/EditText_bg"
                    android:singleLine="true"
                    android:inputType="phone"
                    android:maxLength="4"
                    android:digits="0123456789,."
                    android:imeOptions="actionNext"
                    android:layout_height="wrap_content" />

                <EditText
                    android:id="@+id/textFahrstreifenAnzahl2"
                    android:layout_width="0dp"
                    android:layout_weight="0.1"
                    style="@style/EditText_bg"
                    android:singleLine="true"
                    android:inputType="phone"
                    android:digits="0123456789"
                    android:imeOptions="actionNext"
                    android:layout_height="wrap_content" />

                <EditText
                    android:id="@+id/textFahrbahnbreite2"
                    android:layout_width="0dp"
                    android:layout_weight="0.1"
                    style="@style/EditText_bg"
                    android:singleLine="true"
                    android:inputType="phone"
                    android:maxLength="4"
                    android:digits="0123456789,."
                    android:imeOptions="actionNext"
                    android:layout_height="wrap_content" />

                <EditText
                    android:id="@+id/textGehwegbreite2"
                    android:layout_width="0dp"
                    android:layout_weight="0.1"
                    style="@style/EditText_bg"
                    android:singleLine="true"
                    android:inputType="phone"
                    android:maxLength="4"
                    android:digits="0123456789,."
                    android:imeOptions="actionNext"
                    android:layout_height="wrap_content" />

                <EditText
                    android:id="@+id/textRWBauzeit1"
                    android:layout_width="0dp"
                    android:layout_weight="0.1"
                    style="@style/EditText_bg"
                    android:singleLine="true"
                    android:inputType="phone"
                    android:maxLength="4"
                    android:digits="0123456789,."
                    android:imeOptions="actionNext"
                    android:layout_height="wrap_content" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="0.2"
                    style="@style/EditText_bg"
                    android:gravity="center|left"
                    android:text="During Construction Period"
                    android:layout_height="wrap_content"
                    android:id="@+id/labelDuringConstruction" />

                <EditText
                    android:id="@+id/textFahrstreifenBauzeit1"
                    android:layout_width="0dp"
                    android:layout_weight="0.1"
                    style="@style/EditText_bg"
                    android:singleLine="true"
                    android:inputType="phone"
                    android:digits="0123456789"
                    android:imeOptions="actionNext"
                    android:layout_height="match_parent" />

                <EditText
                    android:id="@+id/textFBBauzeit1"
                    android:layout_width="0dp"
                    android:layout_weight="0.1"
                    style="@style/EditText_bg"
                    android:singleLine="true"
                    android:inputType="phone"
                    android:maxLength="4"
                    android:digits="0123456789,."
                    android:imeOptions="actionNext"
                    android:layout_height="match_parent" />

                <EditText
                    android:id="@+id/textGWBauzeit1"
                    android:layout_width="0dp"
                    android:layout_weight="0.1"
                    style="@style/EditText_bg"
                    android:inputType="phone"
                    android:maxLength="4"
                    android:digits="0123456789,."
                    android:singleLine="true"
                    android:imeOptions="actionNext"
                    android:layout_height="match_parent" />

                <EditText
                    android:id="@+id/textRadwegbreite2"
                    android:layout_width="0dp"
                    android:layout_weight="0.1"
                    style="@style/EditText_bg"
                    android:singleLine="true"
                    android:inputType="phone"
                    android:maxLength="4"
                    android:digits="0123456789,."
                    android:imeOptions="actionNext"
                    android:layout_height="match_parent" />

                <EditText
                    android:id="@+id/textFahrstreifenBauzeit2"
                    android:layout_width="0dp"
                    android:layout_weight="0.1"
                    style="@style/EditText_bg"
                    android:singleLine="true"
                    android:inputType="phone"
                    android:digits="0123456789"
                    android:imeOptions="actionNext"
                    android:layout_height="match_parent" />

                <EditText
                    android:id="@+id/textFBBauzeit2"
                    android:layout_width="0dp"
                    android:layout_weight="0.1"
                    style="@style/EditText_bg"
                    android:singleLine="true"
                    android:inputType="phone"
                    android:maxLength="4"
                    android:digits="0123456789,."
                    android:imeOptions="actionNext"
                    android:layout_height="match_parent" />

                <EditText
                    android:id="@+id/textGWBauzeit2"
                    android:layout_width="0dp"
                    android:layout_weight="0.1"
                    style="@style/EditText_bg"
                    android:singleLine="true"
                    android:inputType="phone"
                    android:maxLength="4"
                    android:digits="0123456789,."
                    android:imeOptions="actionNext"
                    android:layout_height="match_parent" />

                <EditText
                    android:id="@+id/textRWBauzeit2"
                    android:layout_width="0dp"
                    android:layout_weight="0.1"
                    style="@style/EditText_bg"
                    android:singleLine="true"
                    android:inputType="phone"
                    android:maxLength="4"
                    android:digits="0123456789,."
                    android:imeOptions="actionNext"
                    android:layout_height="match_parent" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center">

                <TextView
                    android:layout_width="0dp"
                    android:layout_weight="0.2"
                    style="@style/EditText_bg"
                    android:gravity="center|left"
                    android:text="Usually Plan By RSA 95"
                    android:layout_height="wrap_content"
                    android:id="@+id/labelUsuallyPlan" />

                <EditText
                    android:id="@+id/textUsuallyPlane"
                    android:layout_width="0dp"
                    style="@style/EditText_bg"
                    android:layout_weight="0.8"
                    android:singleLine="true"
                    android:imeOptions="actionNext"
                    android:layout_height="wrap_content" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            style="@style/padding_style"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/labelPreparationOfStoppingSigns"
                style="@style/TextView_Black"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="Preparation of Stopping Signs " />

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center">

                <ToggleButton
                    style="@style/TextView_White"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/toggle_selector"
                    android:checked="true"
                    android:textOff=""
                    android:textOn=""
                    android:id="@+id/button8" />
            </LinearLayout>

            <TextView
                android:id="@+id/labelNote"
                style="@style/TextView_Black"
                android:layout_width="0dp"
                android:layout_weight="0.5"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:text="Note" />

            <EditText
                android:id="@+id/textNote"
                style="@style/EditText_bg"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:singleLine="true"
                android:maxLength="50"
                android:layout_weight="3.5" />
        </LinearLayout>

        <LinearLayout
            style="@style/padding_style"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal">

            <CheckBox
